CREATE TABLE IF NOT EXISTS FINISHED_JOBS_STG (id String,name String, queue String, username String, state String, submitTime bigint, startTime bigint, finishTime bigint,avgMapTime bigint, avgReduceTime bigint, avgShuffleTime bigint, avgMergeTime bigint, gcTime bigint, usedMemory bigint, timeSpentMaps bigint, timeSpentReducers bigint, timeSpentTotal bigint, totalFileBytesRead   bigint ,totalFileBytesWritten  bigint ,totalFileReadOps  bigint ,totalFileLargeReadOps  bigint ,totalFileWriteOps  bigint ,totalHDFSBytesRead  bigint ,totalHDFSBytesWritten  bigint ,totalHDFSReadOps  bigint ,totalHDFSLargeReadOps  bigint ,totalHDFSWriteOps bigint, fetchTime TIMESTAMP )
ROW FORMAT DELIMITED
FIELDS TERMINATED BY '\t'
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;

CREATE TABLE IF NOT EXISTS FINISHED_JOBS (id String,name String, queue String, username String, state String, submitTime bigint, startTime bigint, finishTime bigint,avgMapTime bigint, avgReduceTime bigint, avgShuffleTime bigint, avgMergeTime bigint, gcTime bigint, usedMemory bigint, timeSpentMaps bigint, timeSpentReducers bigint, timeSpentTotal bigint,totalFileBytesRead   bigint ,totalFileBytesWritten  bigint ,totalFileReadOps  bigint ,totalFileLargeReadOps  bigint ,totalFileWriteOps  bigint ,totalHDFSBytesRead  bigint ,totalHDFSBytesWritten  bigint ,totalHDFSReadOps  bigint ,totalHDFSLargeReadOps  bigint ,totalHDFSWriteOps bigint, fetchTime TIMESTAMP )
PARTITIONED BY (date string, hour string)
ROW FORMAT DELIMITED
FIELDS TERMINATED BY '\t'
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;



CREATE TABLE IF NOT EXISTS RUNNING_JOBS_STG (applicationId String,applicationName String, applicationState String, applicationType String, finalState String, progress String, username String, queueName String , startTime bigint, elapsedTime bigint, finishTime bigint, trackingUrl String, numContainers int, allocatedMB int, allocatedVCores int, memorySeconds bigint, vcoreSeconds bigint , fetchTime TIMESTAMP)
ROW FORMAT DELIMITED
FIELDS TERMINATED BY '\t'
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;

CREATE TABLE IF NOT EXISTS RUNNING_JOBS (applicationId String,applicationName String, applicationState String, applicationType String, finalState String, progress String, username String, queueName String , startTime bigint, elapsedTime bigint, finishTime bigint, trackingUrl String, numContainers int, allocatedMB int, allocatedVCores int, memorySeconds bigint, vcoreSeconds bigint , fetchTime TIMESTAMP)
PARTITIONED BY (date string, hour string)
ROW FORMAT DELIMITED
FIELDS TERMINATED BY '\t'
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;



CREATE TABLE IF NOT EXISTS QUEUES_STG (queueName String, absoluteAllocatedCapacity double, absoluteUsedCapacity double, usedMemory int, usedCores int, numContainers int, queueState String, maxApplications int, numApplications int, numActiveApplications int, numPendingApplications int , queueType String, users String, fetchTime TIMESTAMP)
PARTITIONED BY (date string, hour string)
ROW FORMAT DELIMITED
FIELDS TERMINATED BY '\t'
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;

CREATE TABLE IF NOT EXISTS QUEUES (queueName String, absoluteAllocatedCapacity double, absoluteUsedCapacity double, usedMemory int, usedCores int, numContainers int, queueState String, maxApplications int, numApplications int, numActiveApplications int, numPendingApplications int , queueType String, users String, fetchTime TIMESTAMP)
PARTITIONED BY (date string, hour string)
ROW FORMAT DELIMITED
FIELDS TERMINATED BY '\t'
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;